{
    "id": "org.kde.tellico",
    "command": "tellico",
    "runtime": "org.kde.Platform",
    "runtime-version": "5.15-21.08",
    "base": "io.qt.qtwebengine.BaseApp",
    "base-version": "5.15-21.08",
    "sdk": "org.kde.Sdk",
    "rename-icon": "tellico",
    "finish-args": [
        "--share=ipc",
        "--share=network",
        "--socket=fallback-x11",
        "--socket=wayland",
        "--device=dri"
    ],
    "modules": [
        {
            "name": "taglib",
            "buildsystem": "cmake-ninja",
            "config-opts": [
                "-DBUILD_SHARED_LIBS=ON"
            ],
            "sources": [
                {
                    "type": "archive",
                    "url": "https://taglib.github.io/releases/taglib-1.13.tar.gz",
                    "sha256": "58f08b4db3dc31ed152c04896ee9172d22052bc7ef12888028c01d8b1d60ade0",
                    "x-checker-data": {
                        "type": "anitya",
                        "project-id": 1982,
                        "stable-only": true,
                        "url-template": "https://taglib.github.io/releases/taglib-$version.tar.gz"
                    }
                }
            ]
        },
        {
            "name": "yaz",
            "buildsystem": "autotools",
            "sources": [
                {
                    "type": "archive",
                    "url": "https://ftp.indexdata.com/pub/yaz/yaz-5.33.0.tar.gz",
                    "sha256": "9eab77267524191a8286ad80291a2220ffe9d322b3ea0e4b1c6bdbc5db21a04f",
                    "x-checker-data": {
                        "type": "anitya",
                        "project-id": 5287,
                        "stable-only": true,
                        "url-template": "https://ftp.indexdata.com/pub/yaz/yaz-$version.tar.gz"
                    }
                }
            ]
        },
        {
            "name": "tellico",
            "buildsystem": "cmake-ninja",
            "config-opts": [
                "-DBUILD_TESTING=FALSE",
                "-DCMAKE_BUILD_TYPE=RelWithDebInfo"
            ],
            "sources": [
                {
                    "type": "archive",
                    "url": "https://tellico-project.org/files/tellico-3.4.5.tar.xz",
                    "sha256": "39a8bd38d2d3f77a37deae27d9587263c0c2b2935c82108bfd143a0fc7cbebcd",
                    "x-checker-data": {
                        "type": "anitya",
                        "project-id": 4950,
                        "stable-only": true,
                        "url-template": "https://tellico-project.org/files/tellico-$version.tar.xz"
                    }
                }
            ]
        }
    ]
}
